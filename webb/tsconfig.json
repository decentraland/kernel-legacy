{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "~/*": ["./*"],
      "@dcl/webb/*": ["./*"],
      "@dcl/utils/*": ["../../utils/*"],
      "@dcl/utils": ["../../utils"],
      "@dcl/client": ["../../client"],
      "@dcl/client/*": ["../../client/*"],
      "@dcl/config": ["../../config"],
      "@dcl/config/*": ["../../config/*"]
    },
    "downlevelIteration": true
  }
}
