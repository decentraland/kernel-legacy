{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "@dcl/*": ["bazel-bin/*", "bazel-genfiles/*", "*"],
      "dcl/*": ["bazel-bin/*", "bazel-genfiles/*", "*"]
    },
    "downlevelIteration": true
  },
  "extends": "../tsconfig.json"
}
