{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "jsx": "react",
    "diagnostics": false,
    "moduleResolution": "node",
    "stripInternal": true,
    "inlineSourceMap": false,
    "downlevelIteration": true,
    "inlineSources": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "declaration": true,
    "preserveConstEnums": true,
    "incremental": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "removeComments": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "baseUrl": "./webb/src",
    "paths": {
      "~/*": ["*"],
      "dcl/*": ["*", "bazel-bin/*", "bazel-genfiles/*"],
      "@dcl/*": ["*", "bazel-bin/*", "bazel-genfiles/*"]
    },
  }
}